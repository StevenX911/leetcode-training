{
  "name": "leetcode-training",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/StevenX911/leetcode-training.git"
  },
  "scripts": {
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "test": "mocha",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:create": "eslint --init",
    "lint-staged": "lint-staged",
    "coverage": "nyc npm run test",
    "prettier:check": "prettier --check src",
    "prettier:write": "prettier --write src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "src/*.js": [
      "prettier",
      "eslint"
    ]
  },
  "engines": {
    "node": "^8 | ^10 | ^12"
  },
  "keywords": [
    "leetcode",
    "training"
  ],
  "author": "Steven",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^14.14.9",
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "eslint": "^7.14.0",
    "eslint-plugin-ghost": "^2.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "2.2.1"
  }
}
